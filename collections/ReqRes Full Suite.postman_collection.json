        {
          "name": "Single User",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "// CSV gives us strings, so parse the status to a number",
                  "const expectedStatus = parseInt(pm.iterationData.get('expectedStatus'));",
                  "const expectedName   = pm.iterationData.get('expectedFirstName');",
                  "",
                  "pm.test(`Status is ${expectedStatus}`, () => {",
                  "    pm.response.to.have.status(expectedStatus);",
                  "});",
                  "",
                  "pm.test(\"Response time under 800ms\", () => {",
                  "    pm.expect(pm.response.responseTime).to.be.below(800);",
                  "});",
                  "",
                  "// Only run these checks when we expect a real user in the response",
                  "if (expectedStatus === 200) {",
                  "    pm.test(\"Schema is valid\", () => {",
                  "        const schema = {",
                  "            type: \"object\",",
                  "            required: [\"data\", \"support\"],",
                  "            properties: {",
                  "                data: {",
                  "                    type: \"object\",",
                  "                    required: [\"id\", \"email\", \"first_name\", \"last_name\", \"avatar\"]",
                  "                }",
                  "            }",
                  "        };",
                  "        pm.response.to.have.jsonSchema(schema);",
                  "    });",
                  "",
                  "    pm.test(\"Email is from reqres.in domain\", () => {",
                  "        const { data } = pm.response.json();",
                  "        pm.expect(data.email).to.match(/@reqres\\.in$/);",
                  "    });",
                  "",
                  "    pm.test(`First name is ${expectedName}`, () => {",
                  "        pm.expect(pm.response.json().data.first_name).to.eql(expectedName);",
                  "    });",
                  "}"
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            },
            {
              "listen": "prerequest",
              "script": {
                "packages": {},
                "type": "text/javascript"
              }
            }
