                "id": "799962b0-c9e7-4b1d-98e5-aaa4f7a0b443",
                "exec": [
                  "pm.test(\"Status is 201\", () => {",
                  "    pm.response.to.have.status(201);",
                  "});",
                  "",
                  "pm.test(\"Response time under 800ms\", () => {",
                  "    pm.expect(pm.response.responseTime).to.be.below(800);",
                  "});",
                  "",
                  "pm.test(\"Response has id and createdAt\", () => {",
                  "    const json = pm.response.json();",
                  "    pm.expect(json).to.have.property('id');",
                  "    pm.expect(json).to.have.property('createdAt');",
                  "});",
                  "",
                  "pm.test(\"Name and job are correct\", () => {",
                  "    const json = pm.response.json();",
                  "    pm.expect(json.name).to.eql(\"morpheus\");",
                  "    pm.expect(json.job).to.eql(\"leader\");",
                  "});",
                  "",
                  "// Save the id for Update and Delete to use",
                  "pm.environment.set('createdUserId', pm.response.json().id);"
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
